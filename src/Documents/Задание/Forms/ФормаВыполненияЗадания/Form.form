      <fields xsi:type="schema:DataCompositionSchemaDataSetField">
        <dataPath>Уценка</dataPath>
        <field>Уценка</field>
      </fields>
      <calculatedFields>
        <dataPath>МодельПредставление</dataPath>
        <expression>Модель</expression>
        <presentationExpression>ВЫБОР
КОГДА Уценка = 0 ТОГДА Модель.Наименование
ИНАЧЕ Модель.Наименование + &quot;, УЦЕНКА &quot; + СТРОКА(Уценка) + &quot;%&quot;
КОНЕЦ</presentationExpression>
      </calculatedFields>
      <calculatedFields>
        <dataPath>РазмерПредставление</dataPath>
        <expression>Размер</expression>
        <presentationExpression>ВЫБОР
КОГДА Размер = ЗНАЧЕНИЕ(Справочник.моделиРазмер.ПустаяСсылка) ТОГДА &quot;КОРОБ&quot;
ИНАЧЕ &quot;РОССЫПЬ, &quot; + Размер.Наименование
КОНЕЦ</presentationExpression>
      </calculatedFields>
