      <fields xsi:type="schema:DataCompositionSchemaDataSetField">
        <dataPath>Цвет</dataPath>
        <field>Цвет</field>
      </fields>
      <fields xsi:type="schema:DataCompositionSchemaDataSetField">
        <dataPath>Количество</dataPath>
        <field>Количество</field>
        <title>
          <localValue>
            <content>
              <key>ru</key>
              <value>Количество</value>
            </content>
          </localValue>
        </title>
