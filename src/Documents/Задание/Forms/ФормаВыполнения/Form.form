        <dataPath>Уценка</dataPath>
        <field>Уценка</field>
      </fields>
      <calculatedFields>
        <dataPath>ПредставлениеМодели</dataPath>
        <expression>Модель</expression>
        <useRestriction>
          <condition>true</condition>
          <group>true</group>
          <order>true</order>
        </useRestriction>
        <presentationExpression>ВЫБОР
КОГДА Уценка = 0 ТОГДА Модель.Наименование
ИНАЧЕ &quot;(&quot; + СТРОКА(Уценка) + &quot;%) &quot; + Модель.Наименование
КОНЕЦ</presentationExpression>
      </calculatedFields>
      <calculatedFields>
        <dataPath>ПредставлениеРазмера</dataPath>
