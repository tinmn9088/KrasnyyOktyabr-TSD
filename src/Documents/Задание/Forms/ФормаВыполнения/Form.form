    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
    ВсеЗаданияНаОтбор.Количество,
    ВсеЗаданияНаОтбор.Модификатор,
    ВсеЗаданияНаОтбор.Модель КАК Модель,
    ВсеЗаданияНаОтбор.Размер,
    ВсеЗаданияНаОтбор.Уценка,
    ВсеЗаданияНаОтбор.Цвет,
    ВсеЗаданияНаОтбор.Ячейка
ПОМЕСТИТЬ ЗаданияНаОтбор
ИЗ
    Документ.Задание.ЗаданиеНаОтбор КАК ВсеЗаданияНаОтбор
ГДЕ
    ВсеЗаданияНаОтбор.Ссылка = &amp;Задание
    И ВсеЗаданияНаОтбор.Комплектовщик = &amp;Комплектовщик
    И ВсеЗаданияНаОтбор.Количество > 0

ИНДЕКСИРОВАТЬ ПО
    Модель
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
    ЗаданияНаОтбор.Ячейка,
    ЗаданияНаОтбор.Модель,
    ЗаданияНаОтбор.Размер,
    ЗаданияНаОтбор.Модификатор,
    ЗаданияНаОтбор.Цвет,
    ВЫБОР
        КОГДА ЗаданияНаОтбор.Размер &lt;> ЗНАЧЕНИЕ(Справочник.моделиРазмер.ПустаяСсылка)
            ТОГДА ЗаданияНаОтбор.Количество
        ИНАЧЕ ЗаданияНаОтбор.Количество / (Модели.КолВКоробе * (1 + Модели.флДваКороба))
    КОНЕЦ КАК Количество,
    ЗаданияНаОтбор.Уценка
ИЗ
    ЗаданияНаОтбор КАК ЗаданияНаОтбор
        ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.Модели КАК Модели
        ПО ЗаданияНаОтбор.Модель = Модели.Ссылка</queryText>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <fields xsi:type="schema:DataCompositionSchemaDataSetField">
